from requests import get, delete

# список новостей сначала
print(get('http://localhost:800/api/jobs').json())

# некорректный | новости с id = 999 нет в базе
print(delete('http://localhost:800/api/jobs/999').json())

# неверный формат
print(delete('http://localhost:800/api/jobs/Victor').json())

# корректный
print(delete('http://localhost:5000/api/jobs/1').json())

# список новостей в конце
print(get('http://localhost:800/api/jobs').json())
